<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ur.mi.android.wgplus05">


    <uses-feature android:name="android.hardware.camera"
        android:required="true"/>
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.hardware.camera.autofocus" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WAKE_LOCK"/>


    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_icon_hdx"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_icon_hdx"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.DarkActionBar">
        <activity android:name=".MainActivity">



            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name=".Finanzen.Finanzen" />
        <activity android:name=".Einkaufsliste_alt" />
        <activity android:name=".Einkaufsliste.Einkaufsliste2" />
        <activity android:name=".toDoList" />
        <activity android:name=".Einstellungen.Settings" />
        <activity android:name=".FotoWand.PictureActivity" />
        <activity
            android:name=".Putzplan.Putzplan"
            android:windowSoftInputMode="adjustResize" />
        <activity android:name=".Kalender.Kalender" />
        <provider
            android:name=".FotoWand.GenericFileProvider"
            android:authorities="${applicationId}.genericfileprovider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/provider_paths"/>
        </provider>

        <receiver android:name=".Kalender.AlarmNotificationReceiver"/>
    </application>

</manifest>