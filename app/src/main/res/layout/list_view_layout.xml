<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/layout_round_corners_dark"
    android:layout_margin="5dp"
    >

        <ImageView
            android:id="@+id/img_list_view"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:background="@drawable/layout_round_corners"
            android:layout_margin="10dp"/>


        <TextView
            android:id="@+id/titel_list_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="bad putzen"
            android:layout_marginLeft="100dp"
            android:layout_marginTop="15dp"
            android:textSize="20sp"
            />



        <TextView
            android:id="@+id/date_list_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="01.01.2001"
            android:layout_gravity="bottom|left"
            android:layout_marginLeft="100dp"
            android:layout_marginBottom="15dp"
            android:textSize="20sp"/>



        <TextView
            android:id="@+id/aufwand_list_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="aufwand"
            android:layout_gravity="end"
            android:layout_margin="15dp"
            android:textSize="20sp"
            />



        <TextView
            android:id="@+id/frequenz_list_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="wÃ¶chentlich"
            android:layout_gravity="end|bottom"
            android:layout_margin="15dp"
            android:textSize="20sp"
            />






</FrameLayout>